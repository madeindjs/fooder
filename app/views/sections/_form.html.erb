<%= bootstrap_form_for section do |f| %>
  <%= render 'shared/errors_form', model: section %>

  <%= f.text_field :title, label: 'Titre' %>
  <%= f.text_area :content, label: 'Contenu' %>

  <%= render 'shared/form_input_picture.html', form: f, model: section %>

  <%= f.submit 'Confirmer' %>
<% end %>
