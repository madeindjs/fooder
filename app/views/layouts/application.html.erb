<!DOCTYPE html>
<html lang="en">
  <head>
    <%= render 'layouts/meta' %>
    <%= csrf_meta_tags %>
    <!-- Bootstrap core CSS -->
    <% if @restaurant && @restaurant.css %>
      <%= stylesheet_link_tag @restaurant.css %>
    <% else %>
      <%= stylesheet_link_tag 'https://maxcdn.bootstrapcdn.com/bootswatch/3.3.7/readable/bootstrap.min.css' %>
    <% end %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
  <body>
    <div class="container main-container container-fluid">
      <%= render 'layouts/navigation' %>
      <div class="row">
        <div
          id="main-container"
          style="padding-top: 50px">
          <%= render 'shared/flash' %>
          <%= yield %>
        </div>
        <!-- /.container -->
      </div>
    </div>
    <%= render 'layouts/footer' %>
    <noscript id="deferred-styles">
      <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.css">
    </noscript>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
  </body>
</html>