<div class="form-group">
  <%= form.label :logo  %>
  <%= image_tag model.logo.url, size:50,  class: 'img-circle' if model.logo? %>
  <%= form.file_field :logo %>
</div>

<div class="form-group">
  <%= form.label :logo_display, 'Affichage'  %>
  <%= form.select :logo_display,  display_options, {}, class: 'form-control' %>
</div>